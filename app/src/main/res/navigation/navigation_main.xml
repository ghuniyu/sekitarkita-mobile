<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation_main"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.linkensky.ornet.presentation.welcome.WelcomeFragment"
        android:label="welcomeFragment">
        <action
            android:id="@+id/action_welcomeFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@+id/welcomeFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_welcomeFragment_to_macAddressFragment"
            app:destination="@id/macAddressFragment" />
        <action
            android:id="@+id/action_welcomeFragment_to_selfcheckFragment"
            app:destination="@id/selfcheckFragment" />
    </fragment>

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.linkensky.ornet.presentation.home.HomeFragment"
        android:label="HomeFragment">
        <action
            android:id="@+id/action_homeFragment_to_interactionFragment"
            app:destination="@id/interactionFragment" />
        <action
            android:id="@+id/action_homeFragment_to_macAddressFragment"
            app:destination="@id/macAddressFragment" />
        <action
            android:id="@+id/action_homeFragment_to_selfcheckFragment"
            app:destination="@id/selfcheckFragment" />
        <action
            android:id="@+id/action_homeFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
        <action
            android:id="@+id/action_homeFragment_to_createSikmFragment"
            app:destination="@id/createSikmFragment" />
    </fragment>

    <fragment
        android:id="@+id/interactionFragment"
        android:name="com.linkensky.ornet.presentation.interaction.InteractionFragment"
        android:label="interactionFragment">
        <action
            android:id="@+id/action_interactionFragment_to_statisticFragment"
            app:destination="@id/statisticFragment" />
        <action
            android:id="@+id/action_interactionFragment_to_macAddressFragment"
            app:destination="@id/macAddressFragment" />
        <action
            android:id="@+id/action_interactionFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
        <action
            android:id="@+id/action_interactionFragment_to_interactionDetailFragment"
            app:destination="@id/interactionDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/statisticFragment"
        android:name="com.linkensky.ornet.presentation.information.InformationFragment"
        android:label="statisticFragment">
        <action
            android:id="@+id/action_statisticFragment_to_reportFragment"
            app:destination="@id/reportFragment" />
        <action
            android:id="@+id/action_statisticFragment_to_macAddressFragment"
            app:destination="@id/macAddressFragment" />
        <action
            android:id="@+id/action_statisticFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
    </fragment>

    <fragment
        android:id="@+id/reportFragment"
        android:name="com.linkensky.ornet.presentation.report.ReportFragment"
        android:label="reportFragment">
        <action
            android:id="@+id/action_reportFragment_to_macAddressFragment"
            app:destination="@id/macAddressFragment" />
        <action
            android:id="@+id/action_reportFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
    </fragment>

    <fragment
        android:id="@+id/macAddressFragment"
        android:name="com.linkensky.ornet.presentation.mac.MacAddressFragment"
        android:label="macAddressFragment">
        <action
            android:id="@+id/action_macAddressFragment_to_selfcheckFragment"
            app:destination="@id/selfcheckFragment"
            app:enterAnim="@anim/fragment_fade_enter" />
        <action
            android:id="@+id/action_macAddressFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment" />
    </fragment>

    <fragment
        android:id="@+id/selfcheckFragment"
        android:name="com.linkensky.ornet.presentation.selfcheck.SelfcheckFragment"
        android:label="selfcheckFragment">
        <action
            android:id="@+id/action_selfcheckFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.linkensky.ornet.presentation.about.AboutFragment"
        android:label="AboutFragment" />
    <fragment
        android:id="@+id/interactionDetailFragment"
        android:name="com.linkensky.ornet.presentation.interaction.detail.InteractionDetailFragment"
        android:label="InteractionDetailFragment" />
    <fragment
        android:id="@+id/informationOnBoardFragment"
        android:name="com.linkensky.ornet.presentation.information.InformationOnBoardFragment"
        android:label="InformationOnBoardFragment" >
        <action
            android:id="@+id/action_informationOnBoardFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
        <action
            android:id="@+id/action_informationOnBoardFragment_to_statisticFragment"
            app:destination="@id/statisticFragment" />
        <action
            android:id="@+id/action_informationOnBoardFragment_to_createSikmFragment"
            app:destination="@id/createSikmFragment" />
    </fragment>
    <fragment
        android:id="@+id/createSikmFragment"
        android:name="com.linkensky.ornet.presentation.information.sikm.CreateSikmFragment"
        android:label="CreateSikmFragment" />
</navigation>